package com.claim.entity;

import javax.persistence.*;

@Entity
@Table(name="address")

public class Address {
@Id
@GeneratedValue(strategy=GenerationType.IDENTITY)
private int id;

@Column(name="street")
private String street;

@Column(name="city")
private String city;

@Column(name="zip")
private int zip;

@Column(name="state")
private String state;
}
